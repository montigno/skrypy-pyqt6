import numpy as np
import matplotlib.pyplot as pl

x = np.linspace(0, 2*np.pi, 64)
y = np.sin(x) 

pl.figure()

n = 20
colors = pl.cm.jet(np.linspace(0,1,n))

for i in range(n):
    pl.plot(x, i*y, color=colors[i])

pl.show()
